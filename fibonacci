import java.util.*;
class Fibonacci
{
	public static void main(String args[])
	{
    		int a=0,b=1,c=1;
    		Scanner sc = new Scanner(System.in);
    		System.out.println("Enter the limit : ");
    		int n = sc.nextInt();
    		System.out.print(a+"\t");
    		for (int count=1;count<n;i++)
    		{
       	 		System.out.print(c + "\t");
      	  		c=a+b;
      	  		a=b;
      	  		b=c;
    		}
	}
}
