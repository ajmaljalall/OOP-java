import java.util.*;
class Factorial
{
	public static void main(String args[])
	{
    		int n1,fact=1,i;
    		Scanner sc = new Scanner(System.in);
    		System.out.println("Enter limit : ");
    		n1 = sc.nextInt();
    		for (i=1;i<=n1;fact=fact*i,i++);
    		System.out.println("Factorial is = " + fact);
	}
}
